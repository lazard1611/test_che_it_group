mixin faq_items(data)
	ul.faq__list
		each item in data
			li.faq_item.js-fade(class=`${item.active ? 'faq_item--open_state' : ''}`)
				button(aria-label="accordion item").faq_item__title.js-faq-item-head!=item.title
					span.faq_item__title_icon
				.faq_item__answer
					.faq_item__answer_in!= item.descr
